{
  "expo": {
    "name": "Amaken",
    "slug": "amaken-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/amakenLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/amakenLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "amaken.app",
      "config": {
        "googleMapsApiKey": "AIzaSyBroBuyKqhKMXSUyQq1_Awwik1AZqU-UI8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "amaken.app",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBroBuyKqhKMXSUyQq1_Awwik1AZqU-UI8"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e3685e78-37a2-44bf-aefd-dd98c55760c0"
      }
    },
    "owner": "amaken-organization",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e3685e78-37a2-44bf-aefd-dd98c55760c0"
    }
  }
}
